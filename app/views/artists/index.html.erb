<h1>Artists#index</h1>
<%= link_to "Add New Artist", new_artist_path %>
<hr / />
<% @artists.each do |artist| %>
  <h3><%= link_to "#{artist.name}", artist_path(artist) %></h3>
  <h6> <%= artist.genre %> </h6>
  <%= link_to "edit", edit_artist_path(artist) %>
  <%= link_to "delete", artist_path(artist), method: :delete %>
  <br />
<% end %>
