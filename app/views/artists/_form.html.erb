<%= form_with(model: @artist, local: true) do |f| %>
<div class='field'>
  <%= f.label :name %>
  <%= f.text_field :name, required: true, focus: true %>
</div>
<div class='field'>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
</div>
<%= f.submit %>
<% end %>
<%= link_to 'Go Back', artists_path %>
